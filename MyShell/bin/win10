#

if [ $# = 1 ] && [ $1 = "shutdown" ]
then
	sudo virsh shutdown win10
	sudo systemctl stop smb.service
	sudo systemctl stop gvtvgpu.service
else
	sudo systemctl start gvtvgpu.service
	sudo systemctl start smb.service
	sudo virsh start win10
	virt-manager
fi
